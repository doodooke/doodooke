{
  "name": "doodooke",
  "version": "3.0.0-alpha.7",
  "scripts": {
    "link": "node ./bin/linkNode.js",
    "preinstall": "yarn config set registry https://registry.npm.taobao.org",
    "bootstrap": "yarn install && lerna bootstrap",
    "bootstrap:production": "yarn install --production && lerna bootstrap",
    "upgrade:yarnlock": "find . -name yarn.lock | xargs rm -rf",
    "clean": "lerna clean -y && rm -rf node_modules",
    "web:dev": "lerna exec --scope doodoo-plugin-web -- npm run dev",
    "web:build": "lerna exec --scope doodoo-plugin-web -- npm run build",
    "web:generate": "lerna exec --scope doodoo-plugin-web -- npm run generate",
    "predev": "node ./bin/linkNode.js",
    "dev": "cross-env NODE_ENV=development NUXT_BUILD=true ./node app.js",
    "preapi:dev": "node ./bin/linkNode.js",
    "api:dev": "cross-env NODE_ENV=development ./node app.js",
    "preapi:docker": "node ./bin/linkNode.js",
    "api:docker": "cross-env NODE_ENV=docker ./node app.js",
    "api:doc": "apidoc -i app -o www/apidoc -t www/template",
    "eslint": "eslint ./",
    "eslint:fix": "eslint --fix ./",
    "migrate": "node ./bin/migrate.js"
  },
  "files": [
    "app.js",
    "pm2.config.js",
    "upgrade.js",
    "rewrite.js",
    "yarn.lock",
    "bin/*",
    "migrations/*"
  ],
  "publishConfig": {
    "registry": "http://registry.doodooke.com"
  },
  "apidoc": {
    "name": "多多客",
    "version": "0.0.1",
    "description": "多多客",
    "title": "多多客",
    "url": "http://127.0.0.1:3000/api",
    "sampleUrl": "http://127.0.0.1:3000/api"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "prettier": "^1.14.3"
  },
  "dependencies": {
    "apidoc": "^0.17.6",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "bluebird": "^3.5.3",
    "clone-response": "^1.0.2",
    "cross-env": "^5.2.0",
    "doodoo-plugin-baas": "^1.0.5-alpha.0",
    "doodoo-plugin-chokidar": "^1.0.1-alpha.5",
    "doodoo-plugin-mysql": "1.0.2",
    "doodoo-plugin-nuxt": "^1.0.2",
    "doodoo-plugin-proxy": "^1.0.1",
    "doodoo-plugin-redis": "^1.0.2",
    "doodoo.js": "^2.0.1-alpha.14",
    "dotenv": "^6.1.0",
    "download": "^7.1.0",
    "glob": "^7.1.3",
    "js-yaml": "^3.12.2",
    "knex": "^0.16.3",
    "koa-cors": "^0.0.16",
    "lerna": "^3.4.3",
    "lodash": "^4.17.11",
    "path-to-regexp": "^3.0.0",
    "progress": "^2.0.3",
    "redis": "^2.8.0",
    "semver": "^5.6.0",
    "shortid": "^2.2.14",
    "socket.io": "^2.1.1",
    "socket.io-redis": "^5.2.0",
    "uuid": "^3.3.2",
    "yargs": "^13.2.2"
  }
}
